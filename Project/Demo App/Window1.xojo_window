#tag Events Upload
	#tag Event
		Sub Pressed()
		  Var f As FolderItem = FolderItem.ShowOpenFileDialog("video/mp4")
		  If f <> Nil Then
		    self.NetImgBBSocket1.PutImage f
		  Else
		    MessageBox("Open failed.")
		  End If
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SetApiKey
	#tag Event
		Sub Pressed()
		  if (not self.APIKey.Text.IsEmpty) then
		    self.NetImgBBSocket1.APIKey = self.APIKey.Text
		    self.APIKey.Enabled = FALSE
		    self.Upload.Enabled = TRUE
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
